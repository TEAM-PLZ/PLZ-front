{
  "printWidth": 100, 
  "semi": true, 
  "singleQuote": true, 
  "trailingComma": "all", 
  "tabWidth": 2,
  "bracketSpacing": true, 
  "endOfLine": "auto",
  "useTabs": false,
  "arrowParens": "avoid",
  "importOrder": [
    "^(react|next).*",
    "^(components).*",
    "^(styles).*",
    "^(services).*",
    "^(hooks).*",
    "^(utils).*",
    "^(types).*",
    "^(stores).*",
    "^.*",
    "^[.].*/.*", 
    ".*"
 ],
 "importOrderSeparation": true,
 "importOrderSortSpecifiers": true
}

